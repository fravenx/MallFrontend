ck=\"toOrder\"\r\n          >直接跳转</el-button\r\n        >\r\n      </template>\r\n    </el-result>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      time: 5,\r\n      timer: null\r\n    }\r\n  },\r\n  created() {\r\n    clearInterval(this.timer)\r\n    this.timer = setInterval(() => {\r\n      if (this.time === 0) {\r\n        clearInterval(this.timer)\r\n        this.$router.push('/order')\r\n      }\r\n      this.time--\r\n    }, 1000)\r\n  },\r\n  methods: {\r\n    cancel() {\r\n      clearInterval(this.timer)\r\n    },\r\n    backToCart() {\r\n      this.$router.push('/cart')\r\n    },\r\n    toOrder() {\r\n      this.$router.push('/order')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.tip {\r\n  color: #606266;\r\n  margin-bottom: 15px;\r\n}\r\n.time {\r\n  font-size: 18px;\r\n  color: red;\r\n}\r\n</style>"]}]}