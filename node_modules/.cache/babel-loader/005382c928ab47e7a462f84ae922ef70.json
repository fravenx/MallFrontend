.response.use(\r\n  response => {\r\n    let res = response.data;\r\n    // 如果是返回的文件\r\n    if (response.config.responseType === 'blob') {\r\n      return res\r\n    }\r\n    // 兼容服务端返回的字符串数据\r\n    if (typeof res === 'string') {\r\n      res = res ? JSON.parse(res) : res\r\n    }\r\n    return res;\r\n  },\r\n  error => {\r\n    console.log('err' + error) // for debug\r\n    return Promise.reject(error)\r\n  }\r\n)\r\n\r\nexport default request\r\n"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AAEA,MAAMC,OAAO,GAAGD,KAAK,CAACE,MAAN,CAAa;EAC3B;EACAC,OAAO,EAAE,uBAFkB;EAG3BC,OAAO,EAAE;AAHkB,CAAb,CAAhB,C,CAMA;AACA;AACA;;AACAH,OAAO,CAACI,YAAR,CAAqBJ,OAArB,CAA6BK,GAA7B,CAAiCC,MAAM,IAAI;EACzCA,MAAM,CAACC,OAAP,CAAe,cAAf,IAAiC,gCAAjC,CADyC,CAEzC;EAEA;EAEA;EACA;EACA;EACA;;EACA;AACF;AACA;AACA;AACA;;EAEE,OAAOD,MAAP;AACD,CAjBD,EAiBGE,KAAK,IAAI;EACV,OAAOC,OAAO,CAACC,MAAR,CAAeF,KAAf,CAAP;AACD,CAnBD,E,CAqBA;AACA;;AACAR,OAAO,CAACI,YAAR,CAAqBO,QAArB,CAA8BN,GAA9B,CACEM,QAAQ,IAAI;EACV,IAAIC,GAAG,GAAGD,QAAQ,CAACE,IAAnB,CADU,CAEV;;EACA,IAAIF,QAAQ,CAACL,MAAT,CAAgBQ,YAAhB,KAAiC,MAArC,EAA6C;IAC3C,OAAOF,GAAP;EACD,CALS,CAMV;;;EACA,IAAI,OAAOA,GAAP,KAAe,QAAnB,EAA6B;IAC3BA,GAAG,GAAGA,GAAG,GAAGG,IAAI,CAACC,KAAL,CAAWJ,GAAX,CAAH,GAAqBA,GAA9B;EACD;;EACD,OAAOA,GAAP;AACD,CAZH,EAaEJ,KAAK,IAAI;EACPS,OAAO,CAACC,GAAR,CAAY,QAAQV,KAApB,EADO,CACoB;;EAC3B,OAAOC,OAAO,CAACC,MAAR,CAAeF,KAAf,CAAP;AACD,CAhBH;AAmBA,eAAeR,OAAf"}]}